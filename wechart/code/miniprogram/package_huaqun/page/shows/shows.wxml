<view class="list">
  <view class="item" hover-class="item_hover" wx:for="{{showList}}" wx:key="text" bindtap="go" data-index="{{index}}">
    {{item.text}}
    <image class="img" src="../../images/qianwang.png"></image>
  </view>
</view>

<van-tabbar active="{{ active }}" bind:change="onChange" active-color="#1296db" inactive-color="#000">
  <van-tabbar-item icon="../../images/work.png">基础配置</van-tabbar-item>
  <van-tabbar-item icon="../../images/paichan.png">订单相关</van-tabbar-item>
</van-tabbar>